{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port=3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "cross-env NODE_ENV=test vitest --config vitest.config.ts",
    "test:coverage": "cross-env NODE_ENV=test vitest run --config vitest.config.ts --coverage",
    "test:coverage:watch": "cross-env NODE_ENV=test vitest --config vitest.config.ts --coverage --watch"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "next": "15.3.2"
  },
  "devDependencies": {
    "typescript": "5.8.3",
    "@types/node": "22.14.1",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "@repo/typescript-config": "workspace:*"
  }
}
